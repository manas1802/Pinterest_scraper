    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "selenium-webdriver": "^4.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/puppeteer": "^5.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
